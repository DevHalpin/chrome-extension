{
    "name": "React Chrome Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "My test extension",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts" :[
        {
            "matches": ["https://example.com/*"],
            "js": ["content.js"]
        },
        {
            "matches": ["<all_urls>"],
            "js": ["popup_content.js"]
        }
    ],
    "permissions": [
        "storage"
    ]

}